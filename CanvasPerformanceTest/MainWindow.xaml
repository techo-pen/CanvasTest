<Window x:Class="CanvasPerformanceTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CanvasPerformanceTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas Name="canvas" HorizontalAlignment="Left" Height="200" Margin="10,10,0,0" VerticalAlignment="Top" Width="400" />
        <Button Name="normalDraw" Content="Button" HorizontalAlignment="Left" Margin="582,9,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox Name="normalTime" HorizontalAlignment="Left" Height="23" Margin="662,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Name="improveDraw" Content="Button" HorizontalAlignment="Left" Margin="582,51,0,0" VerticalAlignment="Top" Width="75" Click="improveDraw_Click"/>
        <TextBox Name="inprovelTime" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="662,48,0,0"/>
        <local:Canvas x:Name="inproveCanvas" HorizontalAlignment="Right" Height="200" Margin="0,209,382,0" VerticalAlignment="Top" Width="400" />
    </Grid>
</Window>
